{
	"name" : "alxarafe/alxarafe",
	"type" : "project",
	"description" : "Alxarafe is a refactoring of Dolibarr ERP & CRM",
	"keywords" : [
		"dolibarr",
		"erp",
		"crm",
		"invoice",
		"sme",
		"proposal",
		"order",
		"stock",
		"agenda"
	],
	"homepage" : "https://www.alxarafe.com",
	"license" : "GPL-3.0-or-later",
	"support" : {
		"issues" : "https://github.com/rsanjoseo/alxarafe/issues",
		"forum" : "https://www.alxarafe.com/forum",
		"wiki" : "https://wiki.alxarafe.com",
		"source" : "https://github.com/rsanjoseo/alxarafe"
	},
	"config" : {
		"vendor-dir" : "htdocs/vendor"
	},
	"require" : {
		"php" : ">=7.4",
		"phpmailer/phpmailer": "*",
		"components/jquery": "3.4.1",
		"components/bootstrap": "3.4.1",
		"components/font-awesome": "4.7.0",
		"mk-j/php_xlsxwriter": "0.37",
		"twbs/bootstrap": "v3.4.1",
		"thomaspark/bootswatch": "v3.4.1",
		"makeusabrew/bootbox": "v5.x",
		"rospdf/pdf-php": "0.12.58",
		"maximebf/debugbar": "v1.15.1",
		"shuchkin/simplexlsx": "0.8.10",
		"parsecsv/php-parsecsv": "1.1.1",
		"mischiefcollective/colorjizz": "1.0.0",
		"twig/twig": "v1.42.3",
		"symfony/http-foundation": "^3.4",
		"symfony/translation": "v3.4.33",
		"symfony/yaml": "v3.4.33",
		"symfony/var-dumper": "v3.4.33",
		"mouse0270/bootstrap-growl": "3.1.3",
		"select2/select2": "3.5.4",
		"ext-mbstring": "*",
		"ext-zip": "*",
		"ext-json": "*",
		"ext-curl": "*",
		"ext-bcmath": "*",
		"ext-simplexml": "*",
		"ext-gd": "*",
		"ext-dom": "*",
		"ext-imagick": "*",
		"ext-memcached": "*"
